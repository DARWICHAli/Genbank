package Utils;

public class Utils {
	private static String ORGANISM_BASE_URL = "https://www.ncbi.nlm.nih.gov/Structure/ngram";
    private static String CDS_BASE_URL = "https://www.ncbi.nlm.nih.gov/sviewer/viewer.fcgi";
    
    public static String getCDSBaseUrl() {
        return CDS_BASE_URL;
    }
}
