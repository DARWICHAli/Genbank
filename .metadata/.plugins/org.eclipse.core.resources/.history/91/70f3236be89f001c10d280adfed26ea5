package src.GUI;
import javax.swing.*;
import static src.GUI.Utils.*;
import java.lang.reflect.Array;
import java.util.Vector;

public final class MenuPanel extends Panel {
	
	private Vector<JCheckBoxMenuItem> items;
	
	MenuPanel(){
		super("menu");
	}


	@Override
	protected void createComponents() {
		for(int i = 0; i < NB_ZONES; i++) {
			items.add(i, new JCheckBoxMenuItem());
			items.elementAt(i).setText("Region" + i.toString());
		}
	}

	@Override
	protected void addComponents() {
		// TODO Auto-generated method stub
		
	}

	@Override
	protected void styleComponents() {
		// TODO Auto-generated method stub
		
	}
}
